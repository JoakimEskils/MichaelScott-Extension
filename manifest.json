{
    "manifest_version": 2,
    "name": "Michael Scott Quotes]",
    "description": "Change all quotes to Michael Scott quotes!",
    "version": "0.1",
    "page_action": {
        "default_icon": {
            "16": "Mike.png",
            "32": "Mike.png",
            "48": "Mike.png",
            "128": "Mike.png"
        }
    },
    "icons": {
        "16": "Mike.png",
        "32": "Mike.png",
        "48": "Mike.png",
        "128": "Mike.png"
    },
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "contentScript.js"
        ],
        "run_at": "document_end"
    }]

}